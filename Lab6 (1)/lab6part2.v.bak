module lab6part2(X1,X2,X3,X4,X5,X6,X7, A,B,C,D)
	input A,B,C,D;
	wire X1,X2,X3,X4,X5,X6,X7
	
	assign X1 = ((~B & ~C & ~D)| (A & ~B & ~C) | (~& B & D) | (~A & C)) ; 
	assign X2 =	((~(A&C&D)|(C&~D)|(A&C)|(A&B));
	assign X3 = ((~B & ~C )| (~B & D) | (~C & D) | (~A & B) | (A & ~B));
	assign X4 =	((A&~C)|(~A&~B&~D)|(~B&C&D)|(B&~C&D)|(B&C&~D) );
	assign X5 =	((~B&~D)|(C&~D)|(A&C)|(A&B));
	assign X6 = ((~A&~B) | (C&~D) | (A&C) | (~B&D) | (~A&~C&D));
	assign X7 = ((A&~B) | (C&~D) | (B&C) | (A&~D) | (~A&~C&D));
	
	
endmodule